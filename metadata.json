{
  "name": "jhoblitt-ipmi",
  "version": "2.3.0",
  "author": "Joshua Hoblitt <jhoblitt@cpan.org>",
  "summary": "manages the OpenIPMI package",
  "license": "Apache-2.0",
  "source": "https://github.com/jhoblitt/puppet-ipmi.git",
